{
  "root": true,
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [],
  "rules": {
    "comma-dangle": [
      2,
      "only-multiline"
    ],
    "indent": [
      2,
      2
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "no-multiple-empty-lines": [
      2,
      {"max": 1}
    ],
    "quotes": [
      2,
      "double"
    ]
  },
  "globals": {}
}
